<h1>Car Listings</h1>
<%= link_to "New Entry", new_car_path %>

<table cellpadding=0, cellspacing=0, border=0, width=100%>
  <th>
    <tr>
      <td>Color</td>
      <td>Year</td>
      <td>Mileage</td>
      <td>Description</td>
    </tr>
  </th>
<% @cars.each do |car| %>
  <tr>
    <td><%= car.color %></td>
    <td><%= car.year %></td>
    <td><%= car.mileage %></td>
    <td><%= car.description %></td>
  </tr>
<% end %>
</table>
